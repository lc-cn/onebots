{
  "name": "onebots",
  "version": "0.4.1",
  "description": "基于icqq的多例oneBot实现",
  "engines": {
    "node": ">=16"
  },
  "main": "lib/index.js",
  "bin": {
    "onebots": "./lib/bin.js"
  },
  "scripts": {
    "start": "node .",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && cp -r src/config.sample.yaml lib/config.sample.yaml",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/bin.ts -c config.yaml",
    "pub": "npm publish --access public",
    "docs:dev": "vitepress dev docs --port 8989",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liucl-cn/onebots.git"
  },
  "keywords": [
    "onebot",
    "icqq",
    "cq-http"
  ],
  "author": "凉菜",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liucl-cn/onebots/issues"
  },
  "homepage": "https://github.com/liucl-cn/onebots#readme",
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/node": "latest",
    "@types/ws": "^8.5.3",
    "ts-node-dev": "latest",
    "tsc-alias": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest",
    "vitepress": "^1.0.0-alpha.72"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/lib/*.yaml",
    "/**/LICENSE"
  ],
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@zhinjs/shared": "^0.0.9",
    "icqq": "^0.3.15",
    "icqq-cq-enable": "^1.0.0",
    "js-yaml": "^4.1.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "log4js": "^6.5.2",
    "ws": "^8.8.0"
  }
}
